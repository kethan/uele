			"require": "./jsx-runtime/dist/index.js"
		},
		"./jsx-dev-runtime": {
			"types": "./jsx-dev-runtime/index.d.ts",
			"browser": "./jsx-dev-runtime/dist/index.es.js",
			"umd": "./jsx-dev-runtime/dist/index.umd.js",
			"import": "./jsx-dev-runtime/dist/index.es.js",
